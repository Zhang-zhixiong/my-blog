body {
    position: relative;
	margin:0;
	padding:0;
	width:100%;
    height: 100%;
	font-family:  Arial, "Microsoft yahei";
    font-weight: 300;
    font-size: 20px;
    line-height: 1.5;
    color: #7f7f7f;
    color: rgba(255, 255, 255, 0.7);
    background: #fff;
	background-size: cover;
    background-attachment: fixed;
    background-repeat: no-repeat;
}
.main-box {
	max-width: 760px;
	min-width: 500px;
    min-height: 720px;
    margin: 5em auto 5em auto;
    position: relative;
    background: rgba(0, 0, 0, 0.85);
    overflow: hidden;
}
.header-menu {
	list-style: none;
    text-align: center;
    margin-bottom: 2em;
    position: relative;
    top: -.5em;
    padding: 0;
    margin: 0;
}
.header-menu li {
	display: inline-block;
    zoom: 1;
    text-align: center;
    padding: 0;
    margin: 0;
}
.header-menu li a {
	padding: 2em 1em 1em 1em;
    position: relative;
    color: rgba(255, 255, 255, 0.5);
    z-index: 10;
    display: -moz-inline-stack;
    display: inline-block;
    zoom: 1;
    -webkit-transition: 0.3s;
    -o-transition: 0.3s;
    transition: 0.3s;
	text-decoration: none;
}
.header-menu li:hover a:after {
	position: absolute;
    content: "";
    height: 117px;
    background: #17e7a4;
    z-index: -1;
    top: 0;
    left: 0;
    width: 100%;
    -webkit-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    -moz-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    -ms-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    -o-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    -ms-border-radius: 7px;
    border-radius: 7px;
}
.header-menu li a:hover {
	color: #fff;
	text-decoration: none;
}
.header-menu li a .icon {
	display: block;
    position: relative;
    font-size: 22px;
    top: 0;
    -webkit-transition: 0.3s;
    -o-transition: 0.3s;
    transition: 0.3s;
}
.header-menu li a:hover .icon {
	top: -10px;
}
.header-menu .active {
	height: 117px;
	-webkit-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    -moz-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    -ms-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    -o-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
	background-color: #17e7a4;
	-webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    -ms-border-radius: 7px;
    border-radius: 7px;
}
.header-menu .active a {
	color: #fff;
	text-decoration: none;
}
.header-menu .active a .icon {
	top: -10px;
}
.header-menu li a .menu-text {
	display: block;
    -webkit-transition: 0.5s;
    -o-transition: 0.5s;
    transition: 0.5s;
}
.content-menu {
	padding-left: 4em;
    padding-right: 4em;
    position: absolute;
    top: 0;
	opacity: 0;
    visibility: hidden;
    width: 100%;
}
.content-menu-inner {
    position: relative;
    float: left;
    width: 100%;
	padding: 7em 2em 4em 2em;
	text-align: center;
}
.content-menu-inner figure .info {
	list-style: none;
}
.content-menu-inner figure .info li {
	text-align: left;
}
.content-menu-inner figure .info li span{
	margin-right: 40px;
}
.content-menu-inner figure .info p {
	display: inline-block;
}
.main-box .footer-menu{
	margin-top: 50px;
    background: rgba(0, 0, 0, 0.5);
    clear: both;
    color: rgba(255, 255, 255, 0.5);
    font-size: 14px;
    position: absolute;
    bottom: 0;
    width: 100%;
    padding: 20px 20px;
    line-height: 1.2;
}
.active {
	opacity: 1;
    visibility: visible;
}
.content-menu .content-menu-inner .info-detail li {
	text-align: left;
	font-size: 16px;
	margin-bottom: 20px;
	color: rgba(255, 255, 255, 0.5);
	list-style-type: circle;
}
.content-menu .content-menu-inner .info-detail li a {
	
	text-decoration: none;
	color: rgba(255, 255, 255, 0.5);
}
.content-menu .content-menu-inner .info-detail li a:hover {
	font-size: 18px;
	-webkit-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    -moz-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    -ms-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    -o-transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
	color: rgba(255, 255, 255, 0.9);
}
.content-menu .content-menu-inner .edu {
	position: relative;
	top: 0;
	left: 0;
	overflow: hidden;
	border-top: 1px solid #b2b2b2;
}
.content-menu .content-menu-inner .edu .img-left {
	width: 120px;
	height: 60px;
	background-color: #b2b2b2;
	position: relative;
	top: -20px;
	left: 0;
	border-radius: 0 0 50px 2px;
}
.content-menu .content-menu-inner .edu .img-left p {
	font-size: 16px;
	position: relative;
	top: 30px;
	left: -15px;
	color: #110f0e;	
	font-weight: 600;
}
.content-menu .content-menu-inner .edu-content .edu-detail li {
	margin-bottom: 10px;
}
.content-menu .content-menu-inner .edu .img-mid {
	width: 140px;
    height: 60px;
    background-color: #b2b2b2;
    position: relative;
    top: -32px;
    left: 35%;
    border-radius: 0 0 30px 30px;
}
.content-menu .content-menu-inner .edu .img-mid p {
	font-size: 16px;
	position: relative;
	top: 35px;
	left: -5px;
	color: #110f0e;	
	font-weight: 600;
}
.indent-in {
	margin-bottom: -10px;
}
.chart {
	width: 160px;
    height: 160px;
    margin: 0 auto 30px auto;
    position: relative;
    text-align: center;
}
.chart span {
	position: absolute;
    top: 50%;
    left: 0;
    margin-top: -30px;
    width: 100%;
}
.chart span strong {
    display: block;
}